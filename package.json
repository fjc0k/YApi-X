{
  "name": "yapi-x",
  "version": "2.5.1",
  "description": "YApi 改。",
  "repository": {
    "type": "git",
    "url": "git@github.com:fjc0k/yapi-x.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jay Fong",
    "email": "fjc0kb@gmail.com",
    "url": "https://github.com/fjc0k"
  },
  "main": "server/app.js",
  "scripts": {
    "build-client": "cross-env NODE_ENV=production ykit pack -m",
    "dev": "concurrently -n sever,client \"npx kill-port 3000 && npm run dev-server\" \"npm run dev-client\"",
    "dev-client": "npm run dev-copy-icon && ykit s -p 4000",
    "dev-copy-icon": "cpr static/iconfont iconfont -d",
    "dev-server": "nodemon server/app.js dev -L",
    "docs": "ydoc build",
    "install-server": "node server/install.js",
    "lint": "eslint **/*.{ts,tsx,js,jsx} --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint --no-error-on-unmatched-pattern --fix",
    "release": "standard-version -a && git push --follow-tags origin master",
    "start": "node server/app.js",
    "test": "ava"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      [
        "webpack-alias",
        {
          "config": "webpack.alias.js"
        }
      ]
    ],
    "presets": [
      [
        "es2015",
        {
          "loose": true,
          "modules": "commonjs"
        }
      ],
      "es2017",
      "stage-0",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "io",
    "rules": {
      "eqeqeq": 0,
      "new-cap": 0,
      "react/jsx-filename-extension": 0,
      "react/sort-comp": 0
    },
    "root": true
  },
  "eslintIgnore": [
    "node_modules",
    "common/json-schema-mockjs.js",
    "dist",
    "/chrome-extension/yapi-x-chrome-extension"
  ],
  "ava": {
    "babel": "inherit",
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "ajv": "^5.5.1",
    "ajv-i18n": "^2.2.0",
    "axios": "0.18.1",
    "compare-versions": "^3.4.0",
